<!--pages/personal/personal.wxml-->
<van-row>
  <van-col span="24">
    <van-cell-group>
      <van-cell  value="{{ orderId }}" title="订单号" icon="coupon-o"></van-cell>
      <van-cell  value="{{ userId }}" title="用户号" icon="user-o"></van-cell>
      <van-cell  value="{{ courierName }}" title="骑手名" icon="user-circle-o"></van-cell>
      <van-cell  value="" title="订单状态" icon="more-o"></van-cell>
      <van-steps steps="{{ steps }}" active="{{ orderStatus }}" />
      <van-cell  value="{{ senderAddress }}" title="订单地址" icon="hotel-o"></van-cell>
      <van-cell  value="{{ senderTel }}" title="电话号码"  icon="location-o"></van-cell>
      <van-cell  value="{{ time }}" title="订单创建时间" icon="clock-o"></van-cell>
      <van-cell  value="{{ senderDate }}" title="预约时间" icon="underway-o"></van-cell>
      <van-cell  value="{{ senderUrgent}}" title="是否加急" icon="bullhorn-o"></van-cell>
      <van-cell  value="{{ integration }}" title="预计获得积分" icon="hotel-o"></van-cell>
    </van-cell-group>
  </van-col>
</van-row>
